//java source code for CodeChef problem HS08TEST

import java.util.Scanner;
 
public class Main{
 
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int x=sc.nextInt();
		float y=sc.nextFloat();
		if(x%5==0&&x<=y-0.50)
			y=y-x-0.50f;
		
		System.out.println(y);
		sc.close();
	}
 
} 
