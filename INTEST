//java source code for CodeChef problem INTEST(a.k.a. Enormous Input Test)

import java.util.Scanner;
 
public class Main {
 
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int k=sc.nextInt();
		int count=0;
		for(int i=1;i<=n;i++){
			int dum=sc.nextInt();
			if(dum%k==0)
				count++;
		}
		System.out.println(count);
		sc.close();
 
	}
 
}
